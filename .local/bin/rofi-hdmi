#!/bin/bash
prompt="ï‰¬"
output="HDMI-A-1"
# output="eDP-1"
# theme_file="$HOME/.config/rofi/theme/gopass.rasi"
mode=$(swaymsg -t get_outputs | jq -jr '.[] | select(type=="object" and .name=="'"$output"'") | .modes|unique_by(.width)|.[] | .width, "x", .height, "\n"' | rofi -dmenu -p "$prompt")
swaymsg output $output mode $mode
